{
  "builds": [
    {
      "src": "submission_project/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "submission_project/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "submission_project.settings",
    "PYTHONPATH": "."
  }
}
